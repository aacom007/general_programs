package smallestpositiveintegertnotinarray;

/**
 *
 * @author akshay anand
 */
public class SmallestPositiveIntegertNotInArray {

    /**
     * @param args the command line arguments
     */
    
    public static int  a[] = {12,1,33,45,3,66,32,444,23,45};
    
    public static void main(String[] args) {
        // TODO code application logic here
        int temp =0, len =0;
        len = a.length-1;
        temp=a[0];
        while(len!=0){
            
            if(a[len]<temp)
                temp= a[len];
            len--;
        }
        
        System.out.print("outpt"+temp);
    }
    
}
